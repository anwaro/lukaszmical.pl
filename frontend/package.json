{
    "name": "lukaszmical_pl",
    "version": "0.1.0",
    "description": "Lukasz Mica≈Ç homepage.",
    "main": "index.js",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "format": "prettier --write \"./**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|md|vue)\"",
        "lint": "eslint \"./**/*.+(js|jsx|ts|tsx)\"",
        "lint:fix": "eslint --fix \"./**/*.+(js|jsx|ts|tsx)\"",
        "code": "yarn format && yarn lint:fix && yarn typescript",
        "tn": "yarn ts-node -r tsconfig-paths/register --project tsconfig.server.json scripts/index.ts",
        "types": "yarn apollo codegen:generate --localSchemaFile=../backend/exports/graphql/schema.graphql --target=typescript --tagName=gql --outputFlat ./src/types",
        "typescript": "tsc --noEmit"
    },
    "author": "anwaro",
    "license": "ISC",
    "repository": "https://github.com/anwaro/lukaszmical.pl",
    "dependencies": {
        "@apollo/client": "3.6.2",
        "apollo": "2.33.11",
        "cross-env": "7.0.3",
        "express": "4.18.1",
        "graphql": "16.5.0",
        "html-loader": "3.1.0",
        "i18next": "21.8.1",
        "isomorphic-unfetch": "3.1.0",
        "mime": "3.0.0",
        "minify": "8.0.4",
        "next": "12.1.6",
        "next-i18next": "11.0.0",
        "node-minify": "3.6.0",
        "nodemon": "2.0.16",
        "prettier": "2.6.2",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "styled-components": "5.3.5"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
        "@types/minify": "6.0.1",
        "@types/node": "17.0.32",
        "@types/react": "18.0.9",
        "@types/react-dom": "18.0.3",
        "@types/styled-components": "5.1.25",
        "@typescript-eslint/eslint-plugin": "5.23.0",
        "@typescript-eslint/parser": "5.23.0",
        "eslint": "8.15.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-import-resolver-babel-module": "5.3.1",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-module-resolver": "1.4.0",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.29.4",
        "ts-node": "10.7.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "4.6.4"
