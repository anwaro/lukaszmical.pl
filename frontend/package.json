        "next": "10.0.7",
        "next-i18next": "^8.0.2",
        "node-minify": "3.6.0",
        "node-sass": "^5.0.0",
        "nodemon": "2.0.7",
        "prettier": "2.2.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-spring": "8.0.27"
